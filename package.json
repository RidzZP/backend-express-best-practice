{
    "name": "express-mvc-template",
    "version": "1.0.0",
    "description": "Express.js MVC Template with Transaction Security, Sequelize, MySQL2, and Winston",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "test": "jest",
        "test:transactions": "node examples/transaction_examples.js",
        "test:excel": "node examples/excel_export_examples.js",
        "db:init": "node src/cli/init-database.js",
        "db:pull": "node src/cli/db-pull.js",
        "db:push": "node src/cli/db-push.js",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
        "db:seed": "npx sequelize-cli db:seed:all",
        "models:clean": "node src/cli/models-clean.js",
        "models:generate": "node src/cli/models-generate.js",
        "user:create-test": "node src/cli/create-test-user.js",
        "docs:serve": "node src/server.js"
    },
    "dependencies": {
        "axios": "^1.6.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "dotenv": "^16.3.1",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.6.5",
        "redis": "^5.5.6",
        "sequelize": "^6.35.2",
        "sequelize-cli": "^6.6.2",
        "sharp": "^0.33.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "supertest": "^6.3.3"
    },
    "keywords": [
        "express",
        "mvc",
        "sequelize",
        "mysql",
        "winston",
        "api",
        "transaction",
        "security",
        "data-integrity",
        "authentication",
        "jwt",
        "excel",
        "export",
        "streaming",
        "swagger",
        "documentation",
        "openapi"
    ],
    "author": "Your Name",
    "license": "MIT"
}
